@using Data
@model IEnumerable<Url>

<h2>MyUrls</h2>

<div class="col-md-offset-3 col-md-6 well">
    <table class="table table-bordered table-striped" id="table">
        <tr>
            <th>Complete Url</th>
            <th>Hashed Url</th>
        </tr>
        @foreach(Url u in Model)
        {
            <tr>
                <td>@u.CompleteUrl</td>
                <td>@u.UrlHash</td>
            </tr>
        }
    </table>
</div>
